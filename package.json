{
  "name": "netflix-clone",
  "version": "1.0.0",
  "description": "Netflix Clone Application",
  "main": "backend/config/server.js",
  "scripts": {
    "start": "node backend/config/server.js",
    "dev": "nodemon backend/config/server.js",
    "test": "jest",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "npm install",
    "install:all": "npm run install:backend && npm run install:frontend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.5.0",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "netflix",
    "clone",
    "streaming",
    "movies",
    "tv-shows"
  ],
